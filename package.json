{
  "name": "belajarelectron",
  "productName": "Belajar Electron",
  "version": "1.0.0",
  "description": "belajar membuat aplikasi elektron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"nge-Electron\"",
    "package-linux": "electron-packager . belajarelectronnn --overwrite --asar --platform=linux --arch=x64 --icon=~/JS/belajarelectron/assets/icons/png/icon.png --prune=true --out=release-builds",
    "dist":"build",
    "pack":"build --dir"
  },
  "build":{
    "appId":"com.catatan.aplikasi",
    "productName":"Aplikasi Catatan"
  },
  "author": "Erlangga <https://erlangga.github.io>",
  "license": "ISC",
  "keywords":[
    "Electron",
    "learning",
    "start",
    "belajar"
  ],
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-packager": "^12.1.0",
    "electron-reload": "^1.2.2"
  },
  "dependencies": {
    "node-localstorage": "^1.3.1"
  }
}
